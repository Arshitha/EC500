{
  "manifest_version": 2,
  "name": "URL Analyser",
  "description": "This extension allows the user to get data about the current tab she is on.",
  "background":{
    "scripts":["background.js"],
    "persistent":false
  },
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_popup": "popup.html",
    "scripts": ["jquery.min.js", "newPopup.js"],
    "default_title": "URL Analyser"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://localhost/27017"
  ]
}